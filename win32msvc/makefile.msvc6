install.nsi : ..\win32msvc/nsis/install.nsi config.status
	sed -e 's/\@NAME\@/$(PACKAGE_NAME)/g' -e 's/\@VERSION\@/$(VERSION)/g' -e 's,\@BASE_MODULES\@,$(BASE_MODULES),g' -e 's,\@MODULES\@,$(MODULES),g' ..\win32msvc/nsis/install.nsi > install.nsi

COPYRIGHT.rtf : COPYRIGHT ..\win32msvc/nsis/text_to_rtf.lisp config.status
	$(RUN) -M lispinit.mem -q ..\win32msvc/nsis/text_to_rtf.lisp COPYRIGHT COPYRIGHT.tmp
	sed -e 's/\@NAME\@/$(PACKAGE_NAME)/g' -e 's/\@VERSION\@/$(VERSION)/g' COPYRIGHT.tmp > COPYRIGHT.rtf
	$(RM) COPYRIGHT.tmp

