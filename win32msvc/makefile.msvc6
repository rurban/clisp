makevars : Makefile
	(echo 'CC='"'"'$(CC)'"'" ;\
	 echo 'CPPFLAGS='"'"'$(CPPFLAGS)'"'" ;\
	 echo 'CFLAGS='"'"'$(CFLAGS)'"'" ;\
	 echo 'CLFLAGS='"'"'$(CLFLAGS)'"'" ;\
	 echo 'LIBS='"'"'lisp.a $(LIBS)'"'" ;\
	 echo 'X_LIBS='"'"'$(X_LIBS)'"'" ;\
