#: spvw.d:695
msgid "Unknown FSUBR signature: %d %d %d\n"
msgstr "Неизвестная сигнатура FSUBR: %d %d %d\n"

#: spvw.d:803
msgid "Unknown SUBR signature: %d %d %d %d"
msgstr "Неизвестная сигнатура SUBR: %d %d %d %d"

#: spvw.d:1479
msgid "module `%s' requires package %s.\n"
msgstr "модуль `%s' требует пакета %s.\n"

#: spvw.d:1518
msgid ""
"is an ANSI Common Lisp.\n"
"Usage:  "
msgstr ""
" - реализация ANSI Common Lisp.\n"
"Использование:  "

#: spvw.d:1520
msgid ""
" [options] [lispfile [argument ...]]\n"
" When `lispfile' is given, it is loaded and `*ARGS*' is set\n"
" to the list of argument strings. Otherwise, an interactive\n"
" read-eval-print loop is entered.\n"
msgstr ""
" [опции] [Лисп-файл [аргумент1 ...]]\n"
" Если задан Лисп-файл, он загружается, при этом параметр `*ARGS*'\n"
" принимает значения списка аргументов. Если файл не задан, происходит\n"
" вход в интерактивный режим `чтение-выполнение-печать'.\n"

#: spvw.d:1524
msgid "Informative output:\n"
msgstr "Информативный вывод:\n"

#: spvw.d:1525
msgid " -h, --help    - print this help and exit\n"
msgstr " -h, --help    - вывод этой справки и выход\n"

#: spvw.d:1526
msgid " --version     - print the version information\n"
msgstr " --version     - вывод информации о версии программы\n"

#: spvw.d:1527
msgid " --license     - print the licensing information\n"
msgstr " --license     - вывод информации о лицензии\n"

#: spvw.d:1528
msgid "Memory image selection:\n"
msgstr "Выбор файла с образом памяти:\n"

#: spvw.d:1529
msgid " -B lisplibdir - set the installation directory\n"
msgstr " -B библ-дир   - задать директорию установки\n"

#: spvw.d:1531
msgid " -K linkingset - use this executable and memory image\n"
msgstr " -K линк-набор - задать исполнимый файл и образ памяти\n"

#: spvw.d:1533
msgid " -M memfile    - use this memory image\n"
msgstr " -M образ      - использовать файл образа памяти\n"

#: spvw.d:1534
msgid " -m size       - memory size (size = xxxxxxxB or xxxxKB or xMB)\n"
msgstr " -m размер     - размер памяти (xxxxxxxB или xxxxKB или xMB)\n"

#: spvw.d:1536
msgid " -s size       - stack size (size = xxxxxxxB or xxxxKB or xMB)\n"
msgstr " -s размер     - размер стека (xxxxxxxB или xxxxKB или xMB)\n"

#: spvw.d:1539
msgid " -t tmpdir     - temporary directory for memmap\n"
msgstr " -t врем-дир   - временная директория для memmap\n"

#: spvw.d:1541
msgid "Internationalization:\n"
msgstr "Интернационализация:\n"

#: spvw.d:1542
msgid " -L language   - set user language\n"
msgstr " -L язык       - задать язык\n"

#: spvw.d:1543
msgid " -N nlsdir     - NLS catalog directory\n"
msgstr " -N nlsdir     - задать каталог с NLS\n"

#: spvw.d:1544
msgid " -Edomain encoding - set encoding\n"
msgstr ""
" -Eтиппотока кодировка - задавать кодировки для различных типов потоков.\n"
"           Осн. типы: file, terminal, pathname, foreign, misc.\n"
"           Осн. кодировки: koi8-r, koi8-ru, koi8-u, CP866, CP1251,\n"
"           ISO-8859-5. См. impnotes.\n"

#: spvw.d:1545
msgid "Interoperability:\n"
msgstr "Взаимодействие:\n"

#: spvw.d:1546
msgid ""
" -q, --quiet, --silent, -v, --verbose - verbosity level:\n"
"     affects banner, *LOAD-VERBOSE*/*COMPILE-VERBOSE*,\n"
"     and *LOAD-PRINT*/*COMPILE-PRINT*\n"
msgstr ""
" -q, --quiet, --silent, -v, --verbose - степень болтливости:\n"
"     приветствие, *LOAD-VERBOSE*/*COMPILE-VERBOSE*,\n"
"     и *LOAD-PRINT*/*COMPILE-PRINT*\n"

#: spvw.d:1549
msgid " -w            - wait for a keypress after program termination\n"
msgstr " -w            - ожидать нажатия клавиши после работы\n"

#: spvw.d:1550
msgid " -I            - be ILISP-friendly\n"
msgstr " -I            - дружить с ILISP\n"

#: spvw.d:1551
msgid "Startup actions:\n"
msgstr "Загрузка:\n"

#: spvw.d:1552
msgid " -ansi         - more ANSI CL compliance\n"
msgstr " -ansi         - большее соответствие стандарту ANSI CL\n"

#: spvw.d:1553
msgid " -traditional  - traditional (undoes -ansi)\n"
msgstr " -traditional  - традиционные установки clisp (отменяет -ansi)\n"

#: spvw.d:1554
msgid " -p package    - start in the package\n"
msgstr " -p пакет      - запускаться в пакете\n"

#: spvw.d:1555
msgid " -C            - set *LOAD-COMPILING* to T\n"
msgstr " -C            - установить *LOAD_COMPILING* в T\n"

#: spvw.d:1556
msgid " -norc         - do not load the user ~/.clisprc file\n"
msgstr " -norc         - не грузить ~/.clisprc\n"

#: spvw.d:1557
msgid " -i file       - load initfile (can be repeated)\n"
msgstr " -i файл       - загрузить файл инициализации (можно повторять)\n"

#: spvw.d:1558
msgid "Actions:\n"
msgstr "Действия:\n"

#: spvw.d:1559
msgid " -c [-l] lispfile [-o outputfile] - compile LISPFILE\n"
msgstr " -c [-l] лисп-файл [-o вых-файл] - компилировать лисп-файл\n"

#: spvw.d:1560
msgid " -x expressions - execute the expressions, then exit\n"
msgstr " -x выражения  - выполнить выражения и завершить работу\n"

#: spvw.d:1561
msgid " lispfile [argument ...] - load lispfile, then exit\n"
msgstr " лисп-файл [аргумент1 ...] - загрузить лисп файл и завершить работу\n"

#: spvw.d:1562
msgid "These actions put CLISP into a batch mode, which is overridden by\n"
msgstr "Эти действия поместят CLISP в пакетный режим, если не задан\n"

#: spvw.d:1563
msgid " -interactive-debug - allow interaction for failed ASSERT and friends\n"
msgstr " -interactive-debug - интерактивное взаимодействие в случае ошибок\n"

#: spvw.d:1564
msgid " -repl              - enter the interactive read-eval-print loop when done\n"
msgstr " -repl             - по окончании войти в интерактивный цикл чтение-выполнение-печать.\n"

#: spvw.d:1565
msgid "Default action is an interactive read-eval-print loop.\n"
msgstr "По умолчанию - интерактивный цикл чтение-выполнение-печать.\n"

