h.lispbibl : c.lispbibl c.fsubr c.subr c.pseudofun c.constsym c.constobj c.acorn h.stdbool h.stdint
	(($(CPP) $(CFLAGS) -P c.lispbibl | $(GREP) -v "^ *$$") ; ($(CPP) $(CFLAGS) -P -dM c.lispbibl | sort) ) > h.lispbibl

gc : h.lispbibl # show GC-related definitions
	egrep '[^[:alpha:]](MEMORY|SPVW|GC|SIGSEGV|SAFETY|ASM|FAST|DEBUG)[^[:alpha:]]' h.lispbibl | grep -v 'define _' | egrep '^#define'


h.clisp : o.genclisph h.unixconf  h.intparam h.floatparam
	$(CC) $(CFLAGS) $(CLFLAGS) o.genclisph -o genclisph
	(echo '#ifndef _CLISP_H' ; echo '#define _CLISP_H' ; echo; echo '/* unixconf */' ; grep '^#' h.unixconf ) > h.clisp
	(echo; echo '/* 'intparam.h' */' ; grep '^#' h.intparam ) >> h.clisp
	(echo; echo '/* 'floatparam.h' */' ; grep '^#' h.floatparam ) >> h.clisp
	(echo; echo '/* genclisph */' ; /@.genclisph ; echo ; echo '#endif /* _CLISP_H */') >> h.clisp
	$(RM) genclisph

linkkit : d.modules c.modules h.clisp
	-$(RMRF) linkkit
	mkdir linkkit
	cd linkkit && $(LN_S) ^.d.modules d.modules
	cd linkkit && $(LN_S) ^.c.modules c.modules
	cd linkkit && $(LN_S) ^.h.clisp h.clisp

fas.exporting : ^.modules.lisp.exporting lisp mem.lispinit
	$(RUN) -M mem.lispinit -q -c ^.modules.lisp.exporting -o fas.exporting

modular : lisp.a  clisp-link linkkit h.modules o.modules makevars
	$(TOUCH) modular

makevars :
	(echo 'CC='"'"'$(CC)'"'" ; echo 'CFLAGS='"'"'$(CFLAGS)'"'" ; echo 'CLFLAGS='"'"'$(CLFLAGS)'"'" ; echo 'LIBS='"'"'lisp.a $(LIBS)'"'" ; echo 'X_LIBS='"'"'$(X_LIBS)'"'" ; echo 'RANLIB='"'"'$(RANLIB)'"'" ; echo 'FILES='"'"'lisp.a '"'") > makevars

clisp-link : in.clisp-link
	sed -e 's%@with_dynamic_modules@%%' -e 's%@createsharedlib@%%' -e 's%@LEXE@%%' < in.clisp-link > clisp-link
	chmod a+x clisp-link

base : modular mem.lispinit
	test -d base || (mkdir base && cd base && for f in lisp.a  h.modules o.modules makevars lisp lispinit.mem; do $(LN_S) ^.$$f $$f; done) || ($(RMRF) base ; exit 1)

anymodule $(MODULES) : lisp mem.lispinit force fas.modprep fas.exporting h.clisp
	test -d $@ || /@.lndir ^.modules/$@ $@
	if test -f $@/configure -a '!' -f $@/config.status ; then cd $@ ; ./configure --cache-file=`echo $@/ | sed -e 's,[^/][^/]*//*,^.,g'`config.cache ; fi
	CLISP="`pwd`.lisp -M `pwd`.mem.lispinit -B `pwd` -Efile UTF-8 -Eterminal UTF-8 -norc" ; cd $@ ; dots=`echo $@/ | sed -e 's,[^/][^/]*//*,^.,g'` ; $(MAKE) clisp-module CC="$(CC)" CFLAGS="$(CFLAGS)" INCLUDES="$$dots" LISPBIBL_INCLUDES=" $${dots}c.lispbibl $${dots}c.fsubr $${dots}c.subr $${dots}c.pseudofun $${dots}c.constsym $${dots}c.constobj $${dots}c.acorn $${dots}h.stdbool $${dots}stdint.h" CLFLAGS="$(CLFLAGS)" LIBS="$(LIBS)" RANLIB="$(RANLIB)" CLISP="$$CLISP -q"

full : modular base $(MODULES)
	$(RMRF) full
	CLISP_LINKKIT=. /@.clisp-link add-module-sets base full $(MODULES) || ($(RMRF) full ; exit 1)

